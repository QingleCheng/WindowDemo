/*time view*/

table.RadCalendarTimeView_ServiceDirect
{
	border-color:#6788be;
	background:#fff;
	font:12px "segoe ui",arial,sans-serif;
}

.RadCalendarTimeView_ServiceDirect th
{
	border-bottom:1px solid #6788be;
	padding:0 0 2px;
	background:#93b4df 0 -1000px repeat-x url('Calendar/sprite.gif');
	color:#fff;
	line-height:22px;
}

table.RadCalendarTimeView_ServiceDirect td
{
	border-color:#6788be;
}

.RadCalendarTimeView_ServiceDirect a
{
	color:#333;
}

.RadCalendarTimeView_ServiceDirect td.rcSelected a
{
	border-color:#85c843;
	background:#d4ffbc;
	color:#0d202b;
}

.RadCalendarTimeView_ServiceDirect td.rcHover a
{
	border-color:#bcd2f1;
	background:#e7f1ff;
	color:#0e3d4f;
}

.RadCalendarTimeView_ServiceDirect .rcFooter
{
	border-color:#6788be;
}

/*pickers*/

.RadPicker_ServiceDirect .rcCalPopup,
.RadPicker_ServiceDirect .rcTimePopup
{
	background-image:url('Calendar/sprite.gif');
}

.RadPicker_ServiceDirect .rcCalPopup,
.RadPicker_ServiceDirect a.rcDisabled.rcCalPopup:hover
{
	background-position:0 0;
}

.RadPicker_ServiceDirect a.rcCalPopup:hover,
.RadPicker_ServiceDirect a.rcCalPopup:focus,
.RadPicker_ServiceDirect a.rcCalPopup:active
{
	background-position:0 -50px;
}

.RadPicker_ServiceDirect .rcTimePopup,
.RadPicker_ServiceDirect a.rcDisabled.rcTimePopup:hover
{
	background-position:0 -100px;
}

.RadPicker_ServiceDirect a.rcTimePopup:hover,
.RadPicker_ServiceDirect a.rcTimePopup:focus,
.RadPicker_ServiceDirect a.rcTimePopup:active
{
	background-position:0 -150px;
}

/*rtl*/

.RadCalendarRTL_ServiceDirect .rcTitlebar .rcFastPrev
{
	background-position:0 -500px;
}

.RadCalendarRTL_ServiceDirect .rcTitlebar a.rcFastPrev:hover
{
	background-position:0 -550px;
}

.RadCalendarRTL_ServiceDirect .rcTitlebar .rcPrev
{
	background-position:0 -400px;
}

.RadCalendarRTL_ServiceDirect .rcTitlebar a.rcPrev:hover
{
	background-position:0 -450px;
}

.RadCalendarRTL_ServiceDirect .rcTitlebar .rcNext
{
	background-position:0 -300px;
}

.RadCalendarRTL_ServiceDirect .rcTitlebar a.rcNext:hover
{
	background-position:0 -350px;
}

.RadCalendarRTL_ServiceDirect .rcTitlebar .rcFastNext
{
	background-position:0 -200px;
}

.RadCalendarRTL_ServiceDirect .rcTitlebar a.rcFastNext:hover
{
	background-position:0 -250px;
}

.RadCalendarMultiViewRTL_ServiceDirect .rcTitlebar a.rcFastPrev:hover
{
	background-position:0 -950px;
}

.RadCalendarMultiViewRTL_ServiceDirect .rcTitlebar a.rcPrev:hover
{
	background-position:0 -850px;
}

.RadCalendarMultiViewRTL_ServiceDirect .rcTitlebar a.rcNext:hover
{
	background-position:0 -750px;
}

.RadCalendarMultiViewRTL_ServiceDirect .rcTitlebar a.rcFastNext:hover
{
	background-position:0 -650px;
}