.RadTabStrip_ServiceDirect .rtsLI,
.RadTabStrip_ServiceDirect .rtsLink
{
	color: #000;
	font: 12px/26px "Segoe UI", Arial, sans-serif;
}

.RadTabStrip_ServiceDirect .rtsLI,
.RadTabStrip_ServiceDirect .rtsLink
{
	color: #fff;
}

.RadTabStrip_ServiceDirect .rtsLink:hover,
.RadTabStrip_ServiceDirect .rtsSelected
{
	color: #0f3789;
}

.RadTabStripLeft_ServiceDirect .rtsSeparator,
.RadTabStripRight_ServiceDirect .rtsSeparator
{
	background: #6788BE;
}

/* <focus cue> */
div.RadTabStrip_ServiceDirect a.rtsLink:focus
{
    outline-color: #000;
}

div.RadTabStrip_ServiceDirect a.rtsLink:focus:after
{
    border-color /*\**/: #000\9;
}
/* </focus cue> */

/* <disabled tabs> */
.RadTabStrip_ServiceDirect .rtsLevel .rtsDisabled,
.RadTabStrip_ServiceDirect .rtsLevel .rtsDisabled:hover
{
	color: #ddd;
}
/* </disabled tabs> */

/* <scrolling arrows> */
.RadTabStrip_ServiceDirect .rtsNextArrow,
.RadTabStrip_ServiceDirect .rtsPrevArrow,
.RadTabStrip_ServiceDirect .rtsPrevArrowDisabled,
.RadTabStrip_ServiceDirect .rtsNextArrowDisabled
{
	background-image: url('TabStrip/TabStripStates.png');
}
/* </scrolling arrows> */

/* <tab backgrounds> */

.RadTabStripTop_ServiceDirect .rtsLevel { background-color: transparent; }

.RadTabStripTop_ServiceDirect .rtsLevel .rtsLink,
.RadTabStripTop_ServiceDirect .rtsLevel .rtsOut,
.RadTabStripBottom_ServiceDirect .rtsLevel .rtsLink,
.RadTabStripBottom_ServiceDirect .rtsLevel .rtsOut,
.RadTabStripTop_ServiceDirect_Baseline .rtsLevel,
.RadTabStripBottom_ServiceDirect_Baseline .rtsLevel { background-image: url('TabStrip/TabStripStates.png'); }

.RadTabStripLeft_ServiceDirect .rtsLink,
.RadTabStripRight_ServiceDirect .rtsLink { background-image: url('TabStrip/TabStripVStates.png'); }

.RadTabStripLeft_ServiceDirect .rtsLast .rtsLink,
.RadTabStripRight_ServiceDirect .rtsLast .rtsLink
{
	border-bottom: 1px solid #898C95;
}

.RadTabStripRight_ServiceDirect .rtsLink,
.RadTabStripRight_ServiceDirect .rtsLI .rtsDisabled:hover { background-position: 100% 0; }
.RadTabStripRight_ServiceDirect .rtsLink:hover { background-position: 100% -200px; }
.RadTabStripRight_ServiceDirect .rtsSelected,
.RadTabStripRight_ServiceDirect .rtsSelected:hover { background-position: 100% -400px; }
/* </tab backgrounds> */


/* <background positioning: tabs with hovers> */

/* <orientation: top> */

.RadTabStripTop_ServiceDirect .rtsLink { background-position: 0 0; }
.RadTabStripTop_ServiceDirect .rtsOut { background-position: 100% 0; }

.RadTabStripTop_ServiceDirect .rtsLink:hover { background-position: 0 -52px; }
.RadTabStripTop_ServiceDirect .rtsLink:hover .rtsOut { background-position: 100% -52px; }

.RadTabStripTop_ServiceDirect .rtsSelected,
.RadTabStripTop_ServiceDirect .rtsSelected:hover { background-position: 0 -26px; }
.RadTabStripTop_ServiceDirect .rtsSelected .rtsOut,
.RadTabStripTop_ServiceDirect .rtsSelected:hover .rtsOut { background-position: 100% -26px; }

.RadTabStripTop_ServiceDirect .rtsDisabled:hover { background-position: 0 0; }
.RadTabStripTop_ServiceDirect .rtsDisabled:hover .rtsOut { background-position: 100% 0; }

.RadTabStripTop_ServiceDirect_Baseline .rtsLevel { background-position: 0 100%; background-repeat: repeat-x; }

/* </orientation: top> */

/* <orientation: bottom> */

.RadTabStripBottom_ServiceDirect .rtsLink { background-position: 0 -182px; }
.RadTabStripBottom_ServiceDirect .rtsOut { background-position: 100% -182px; }

.RadTabStripBottom_ServiceDirect .rtsLink:hover { background-position: 0 -130px; }
.RadTabStripBottom_ServiceDirect .rtsLink:hover .rtsOut { background-position: 100% -130px; }

.RadTabStripBottom_ServiceDirect .rtsSelected,
.RadTabStripBottom_ServiceDirect .rtsSelected:hover { background-position: 0 -156px; }
.RadTabStripBottom_ServiceDirect .rtsSelected .rtsOut,
.RadTabStripBottom_ServiceDirect .rtsSelected:hover .rtsOut { background-position: 100% -156px; }

.RadTabStripBottom_ServiceDirect .rtsDisabled:hover { background-position: 0 -182px; }
.RadTabStripBottom_ServiceDirect .rtsDisabled:hover .rtsOut { background-position: 100% -182px; }

.RadTabStripBottom_ServiceDirect_Baseline .rtsLevel { background-position: 0 -599px; background-repeat: repeat-x; }

/* </orientation: bottom> */

/* <orientation: right> */

.RadTabStripRight_ServiceDirect .rtsUL .rtsLink,
.RadTabStripRight_ServiceDirect .rtsUL .rtsLI .rtsDisabled:hover { background-position: 100% 0; text-align: left; }
.RadTabStripRight_ServiceDirect .rtsUL .rtsLink:hover { background-position: 100% -200px; }
.RadTabStripRight_ServiceDirect .rtsUL .rtsSelected,
.RadTabStripRight_ServiceDirect .rtsUL .rtsSelected:hover { background-position: 100% -400px; }

/* </orientation: right> */

/* </background positioning: tabs with hovers> */

/* <simplistic sub-item styles> */

.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel2,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel3,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel4 { background: #fff; }

.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel2 .rtsLink,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel3 .rtsLink,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel4 .rtsLink
{
	color: #000;
}

.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel2 .rtsLink:hover,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel3 .rtsLink:hover,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel4 .rtsLink:hover
{
	color: #0f3789;
}

.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel2 .rtsSelected,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel3 .rtsSelected,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel4 .rtsSelected
{
	text-decoration: underline;
	color: #0f3789;
}

.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel2 .rtsLink,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel2 .rtsOut,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel2 .rtsIn,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel3 .rtsLink,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel3 .rtsOut,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel3 .rtsIn,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel4 .rtsLink,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel4 .rtsOut,
.RadTabStrip_ServiceDirect_SimpleSubItems .rtsLevel4 .rtsIn
{
	background: none;
}

/* </simplistic sub-item styles> */
