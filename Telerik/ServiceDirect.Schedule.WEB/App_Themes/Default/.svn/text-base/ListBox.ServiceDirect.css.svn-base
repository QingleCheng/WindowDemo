.RadListBox_ServiceDirect .rlbGroup
{
    border: 1px solid #6788be;
    background: #FFF;
}

.RadListBox_ServiceDirect .rlbTemplateContainer
{
    border-color: #6788be;
}

.RadListBox_ServiceDirect .rlbTemplateContainer .rlbGroup
{
    border-top-color: #C7DCF9;
    border-bottom-color: #C6C7D2;
}

.RadListBox_ServiceDirect .rlbText,
.RadListBox_ServiceDirect .rlbItem,
.RadListBox_ServiceDirect .rlbButtonText,
.RadListBox_ServiceDirect .rlbEmptyMessage
{
	font-size: 12px;
	font-family: "Segoe UI", Arial, sans-serif;
	color: #000;
}

.RadListBox_ServiceDirect .rlbHighlight
{
    color: #FFF;
    background-color: #6a8bc1;
}

.RadListBox_ServiceDirect .rlbText,
.RadListBox_ServiceDirect .rlbItem
{
	line-height: 18px;
}

.RadListBox_ServiceDirect .rlbGroup .rlbHovered
{
	background: #e7f1ff;
}

.RadListBox_ServiceDirect .rlbGroup .rlbSelected
{
	background: #d4ffbc;
}

.RadListBox_ServiceDirect .rlbDisabled .rlbText
{
	color: #808080;
}

.RadListBox_ServiceDirect .rlbDisabled .rlbImage
{
	opacity: 0.5;
	-moz-opacity: 0.5;
	filter:alpha(opacity=50);
}

.RadListBox_ServiceDirect .rlbHeader
{
    background-color: #ECF4FF;
}

.RadListBox_ServiceDirect .rlbFooter
{
    background-color: #FFF;
}

.RadListBox_ServiceDirect .rlbDropClue
{
    background-image: url('ListBox/rlbDropClue.png');
    background-position: 0 0;
    background-repeat: no-repeat;
    background-color: transparent;
	left: -10px;
	width: 10px;
}

.RadListBox_ServiceDirect .rlbGroup .rlbDropClueAbove
{
	border-top-style: dotted;
	border-top-color: #8e8e8e;
}

.RadListBox_ServiceDirect .rlbGroup .rlbDropClueBelow
{
	border-bottom-style: dotted;
	border-bottom-color: #8e8e8e;
}

/* Buttons normal state vertical layout*/
.RadListBox_ServiceDirect .rlbButtonText
{
	background: transparent url('ListBox/rlbSprite.png') no-repeat;
}

.RadListBox_ServiceDirect .rlbButton:hover .rlbButtonTL,
.RadListBox_ServiceDirect .rlbButton:hover .rlbButtonTR,
.RadListBox_ServiceDirect .rlbButton:hover .rlbButtonBL,
.RadListBox_ServiceDirect .rlbButton:hover .rlbButtonBR
